RESOURCE REGISTRY_INFO registry_info
{
    resource_format_version = RESOURCE_FORMAT_VERSION_2;
    dll_uid = KAIAgendaPluginEngineDllUid;

    // Interface info array
    interfaces = 
    {
        INTERFACE_INFO
        {
            // UID of the implemented interface
            interface_uid = KAIAgendaPluginEngineIfUid;

            implementations = 
            {
                IMPLEMENTATION_INFO
                {
                    implementation_uid  =  KAIAgendaPluginEngineImplUid;  // UID for ECom interface implementation
                    version_no          =  1;
                }
            };
        }
    };
}


