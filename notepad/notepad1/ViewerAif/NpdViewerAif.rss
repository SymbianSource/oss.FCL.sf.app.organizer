// INCLUDE FILES
#include <aiftool.rh>
#include "NpdViewer.hrh"

//  RESOURCE DEFINITIONS 
// -----------------------------------------------------------------------------
//   
// Application Information resource.
//
// -----------------------------------------------------------------------------
//
RESOURCE AIF_DATA
    {
    app_uid = NPDVIEWER_UID;

    // Number of icons
    //
    num_icons = 2;

    // Application capabilities
    //
    embeddability = KAppEmbeddable;

    hidden=KAppIsHidden;

    datatype_list = 
        {
        DATATYPE
            {
            priority = EDataTypePriorityNormal;
            type = "text/plain";
            }
        };
    }

// End of File
