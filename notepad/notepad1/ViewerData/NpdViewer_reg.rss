#include <appinfo.rh>
#include <NpdViewer.rsg>
#include <data_caging_paths_strings.hrh>

UID2 KUidAppRegistrationResourceFile
UID3 0x1000599d // Define your application UID here

RESOURCE APP_REGISTRATION_INFO
    {
    app_file = "NpdViewer";
    localisable_resource_file = APP_RESOURCE_DIR"\\NpdViewer";
    localisable_resource_id = R_NPDVIEWER_LOCALISABLE_APP_INFO;

    hidden=KAppIsHidden;
    embeddability = KAppEmbeddable;
    datatype_list = 
    	{
       	DATATYPE
       	   {
           priority = EDataTypePriorityNormal;
           type = "text/plain";
           }
        };
    }

