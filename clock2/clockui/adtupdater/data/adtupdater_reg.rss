// System includes
#include <appinfo.rh>

UID2 KUidAppRegistrationResourceFile
UID3 0x2001114C 			// Define your application UID here

RESOURCE APP_REGISTRATION_INFO
    {
    app_file = "adtupdater";
    localisable_resource_file = APP_RESOURCE_DIR"\\adtupdater_loc";
    
    // Need to hide application from app-shell.
    hidden = KAppIsHidden;
    
    // First we'll launch the application in background.
    launch = KAppLaunchInBackground;
    }

// End of File
